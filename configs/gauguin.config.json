{
  "kernelSource": {
    "name": "gauguin",
    "repo": "https://github.com/thofd/neko_kernel_xiaomi_gauguin",
    "branch": "main",
    "device": "gauguin",
    "defconfig": "vendor/gauguin_user"
  },
  "withKernelSU": true,
  "toolchains": [
    {
      "repo": "https://github.com/kdrag0n/proton-clang",
      "branch": "master",
      "name": "proton-clang",
      "binPath": ["bin"]
    }
  ],
          "enableCcache": true, 
          "params":[ 
    { 
            "ARCH": "arm64", 
            "CC": "clang/bin/clang", 
            "externalCommands": { 
              "CROSS_COMPILE": "aarch64-linux-gnu-", 
              "CROSS_COMPILE_ARM32": "arm-linux-gnueabi-", 
              "AS": "clang/bin/llvm-as", 
              "AR": "clang/bin/llvm-ar",  
              "NM":"clang/bin/llvm-nm" ,  
              "OBJCOPY":"clang/bin/llvm-objcopy",  
              "OBJDUMP":"clang/bin/llvm-objdump",  
              "STRIP":"clang/bin/llvm-strip",  
              "LD":"clang/bin/ld.lld", 
              "LTO": "thin" 
            } 
          }, 
          "AnyKernel3": { 
            "use": true, 
            "release": true,
            "custom": {
              "repo": "https://github.com/osm0sis/AnyKernel3",
              "branch": "master"
            }
          },
          "enableKernelSU": true, 
          "KernelSUSFS":{
            "enable": true,
            "branches": "kernel-4.19"
          },
          "enableLXC": true        
    }
]
